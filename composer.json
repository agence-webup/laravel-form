{
    "name": "webup/laravel-form",
    "description": "A Laravel package to help build forms.",
    "keywords": [
        "webup",
        "form",
        "laravel"
    ],
    "homepage": "https://github.com/agence-webup/laravel-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Parisi",
            "email": "robin@agence-webup.com",
            "homepage": "http://agence-webup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Webup\\LaravelForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webup\\LaravelForm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
